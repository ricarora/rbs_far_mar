<form action="/markets/lookup" method="GET">

  <p>
  Market Name: <input id="search" type="text" name="search" />

  <input type="submit"/>

</p>

</form>

<div>

    <% @markets.each do |market| %>
    <ul>
      <li><%= market.name %>, <%= market.address %>, <%= market.city %>, <%= market.state %>, <%= market.zip %> </li>
      <li><%= button_to "Add", add_market_path(market.id) %></li>
    </ul>
    <% end %>

</div>
