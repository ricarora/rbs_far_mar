<form class="new_entry" action="/vendors/<%= @vendor.id %>" method="post">
  <input type="hidden" name="_method" value="PUT">
  <h2>Edit Vendor Profile</h2>

  <div>
    <label for="name">Name(required)</label>
    <input type="text" name="vendor[name]"  value = "<%= @vendor.name %>"  />
 </div>
 <div>
   <label for="no_of_employees">Number of Employees</label>
   <input type="integer" name="vendor[no_of_employees]" value = "<%= @vendor.no_of_employees %>" />
 </div>
 <div>
   <label for="market_name">Market Association</label>
   <input type="integer" name="vendor[market_id]" value = "<%= @vendor.market_id %>"/>
</div>

<br>

   <input type = "submit" value="Save">
 </form>
